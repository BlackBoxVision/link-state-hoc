{
  "name": "link-state-hoc",
  "version": "1.3.1",
  "description": " linkState comes to the rescue of your event handlers!",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf ./dist",
    "compile": "babel ./src -d ./dist",
    "build": "npm run clean && npm run compile",
    "npm-publish": "npm run build && npm publish",
    "test": "mocha ./test --compilers js:babel-register --require babel-polyfill",
    "coverage": "babel-node ./node_modules/.bin/isparta cover _mocha"
  },
  "repository": {
    "url": "https://github.com/BlackBoxVision/link-state-hoc",
    "type": "git"
  },
  "keywords": [
    "react",
    "state-management",
    "link-state"
  ],
  "author": "BlackBox Vision",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BlackBoxVision/link-state-hoc/issues"
  },
  "homepage": "https://github.com/BlackBoxVision/link-state-hoc#readme",
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-polyfill": "^6.23.0",
    "babel-preset-babili": "^0.0.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.23.0",
    "enzyme": "^2.7.1",
    "isparta": "^4.0.0",
    "mocha": "^3.2.0",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.7"
  }
